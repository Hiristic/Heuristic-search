{
  "name": "heuristic-search",
  "version": "1.2.0",
  "license": "Apache-2.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/icons": "^1.0.15",
    "@chakra-ui/react": "^1.6.8",
    "@elastic/react-search-ui": "1.7.0",
    "@elastic/search-ui-app-search-connector": "1.7.0",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "framer-motion": "^4.1.17",
    "prop-types": "^15.7.2",
    "radium": "^0.26.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start": "react-scripts start",
    "start-dev": "npm-run-all -p watch-css start",
    "build-js": "node ./scripts/build-no-chunks.js",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "npm-run-all build-css"
  },
  "devDependencies": {
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "node-sass-chokidar": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "rewire": "^5.0.0"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "browserslist": [
    ">0.2%",
    "chrome 44",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
